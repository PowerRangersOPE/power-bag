{
    "env": {
        "node": true,
        "es6": true
    },
    "extends": [
        "prettier",
        "airbnb-base",
        "eslint:recommended"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
        }
    },
    "rules": {
        "prettier/prettier": "error",
        "comma-dangle": "off",
        "class-methods-use-this": "off",
        "no-unused-expressions": "off",
        "no-unresolved": "off",
        "camelcase": "off",
        "no-useless-catch": "off",
        "array-callback-return": "off",
        "object-curly-newline": "off",
        "no-param-reassign": "off"
    }
}