{
    "env": {
        "commonjs": true,
        "node": true,
        "es6": true
    },
    "extends": [
        "prettier",
        "airbnb-base"
    ],
    "plugins": [
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "rules": {
        "prettier/prettier": "error",
        "comma-dangle": "off",
        "class-methods-use-this": "off",
        "no-unused-expressions": "off"
      }
}
